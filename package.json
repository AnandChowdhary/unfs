{
  "name": "unfs",
  "version": "0.0.0",
  "description": "fs-like bindings for cloud storage services",
  "main": "dist/index.js",
  "types": "dist/index.d.js",
  "files": [
    "dist",
    "src"
  ],
  "repository": "git@github.com:AnandChowdhary/unfs",
  "author": "Anand Chowdhary <mail@anandchowdhary.com>",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "postinstall": "npm-install-peers",
    "test": "jest --coverage",
    "test-report": "jest --coverage --coverageReporters=text-lcov | coveralls"
  },
  "keywords": [
    "typescript",
    "fs",
    "file-system",
    "file-upload",
    "s3",
    "aws",
    "storage",
    "google-cloud",
    "cloud"
  ],
  "devDependencies": {
    "@types/dotenv": "^8.2.0",
    "@types/jest": "^25.1.2",
    "@types/node": "^13.7.1",
    "coveralls": "^3.0.9",
    "dotenv": "^8.2.0",
    "jest": "^25.1.0",
    "npm-install-peers": "^1.2.1",
    "ts-jest": "^25.2.0",
    "typescript": "^3.7.5"
  },
  "peerDependencies": {
    "fs-extra": "^8.1.0",
    "@types/fs-extra": "^8.0.1"
  },
  "jest": {
    "roots": [
      "<rootDir>"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ]
  }
}
